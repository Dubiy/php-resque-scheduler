{
	"name": "mcfedr/php-resque-scheduler",
	"type": "library",
	"description": "php-resque-scheduler is a PHP port of resque-scheduler, which adds support for scheduling items in the future to Resque.",
	"authors": [
		{
			"name": "Chris Boulton",
			"email": "chris@bgigcommerce.com"
		}
	],
	"require": {
		"mcfedr/php-resque": "~1.3"
	},
	"bin": [
		"bin/resque-scheduler"
	],
	"autoload": {
		"psr-0": {
			"ResqueScheduler": "lib/"
		}
	},
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mcfedr/php-resque.git"
        }
    ]
}
